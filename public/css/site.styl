/**********************/
/**********************/
/*    Reset Default   */
/**********************/
/**********************/

body{
    background-color: /*rgba(255,255,255,0.5)*/rgba(238,238,238,0.5);
    margin: 0;
    padding: 0;
}

.row{
    max-width:1140px;
    padding: 0 20px;
}

.top-bar .toggle-topbar.menu-icon a span::after{
    right: 0px;
}

.top-bar .toggle-topbar.menu-icon a{
    padding: 0 2rem 0 0.9375rem;
}

.top-bar,
.top-bar-section li:not(.has-form) a:not(.button),
.top-bar-section li:not(.has-form) a:not(.button):hover,
.top-bar-section ul li,
.top-bar.expanded .title-area,
.no-js .top-bar-section ul li:hover > a{
    background: transparent;
    height: auto;
    line-height:normal;
    padding: 0px;
}

.top-bar .name,
.top-bar .name h1,
.top-bar .name h2,
.top-bar .name h3,
.top-bar .name h4,
.top-bar .name span,
.top-bar .name p{
    line-height: normal;
    height: auto;
    font-size: 1.375rem;
}

.top-bar.expanded .toggle-topbar a{
    color: #fbdd90;
}

.top-bar.expanded .toggle-topbar a span::after {
  box-shadow: 0 0 0 1px #fbdd90, 0 7px 0 1px #fbdd90, 0 14px 0 1px #fbdd90;
}

/************/
/************/
/*  NavBar  */
/************/
/************/

.navbar{
    margin: auto;
    padding: auto;
    position: relative;
    top: 0;
    width: 100%;

    background-color: #a3a3ff;
}

.nav {
  margin-top: 5px;
  margin-bottom: 5px;
  align-items: center;

  overflow: auto;
  list-style-type: none;
}
 
.nav li {
    height: 100%;
    display: inline;
/*    float: right;
*/    margin-top: 25px;
    overflow: hidden;
    /*margin-right: 0px;*/

    padding: 0px auto;
    margin-right: 50px;
    margin-left: auto;
}
 
.nav li:last-child {
  border-right: 10px;
}
 
.nav li a {
  text-decoration: none;
/*color: #58e7dd;*/
  color: #fff;
    
  font: 25px/1 "Roboto-Bold", Verdana, sans-serif;
  text-transform: uppercase;
 
  -webkit-transition: all 0.5s ease;
  -moz-transition: all 0.5s ease;
  -o-transition: all 0.5s ease;
  -ms-transition: all 0.5s ease;
  transition: all 0.5s ease;
}
 
.nav li a:hover {
    color: #333333;
}
 
.nav li.active a {
  font-weight: bold;
  color: #333;
}

.navbar-header img{
    float: center;
    position: relative;

    top: 20px;
    left: 10px;
}

.navbar-brand {
    text-decoration: none;
    font: 85px/1 "Roboto-Bold", Verdana, sans-serif;
    padding: 50px;

    color: #fff;
/*  
    background: -webkit-linear-gradient(#6666ff, #ff4c4c);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
*/
}


