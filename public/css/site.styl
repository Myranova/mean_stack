/*  list team member  */
/**********************/
/**********************/

.pres{
  width: 1300px;
  padding: 0 ;
  margin: 0 auto;
  /*left: 500px;*/
  overflow: auto;
  list-style: none;
}

.pres li{
    float: left;
    margin: 0px auto;
    padding: 0px auto;

    display: -webkit-flex;
    -webkit-flex-direction: column;

    display: flex;
    flex-direction: column;
    float: left;
    margin: 10px auto;
    padding: 10px auto;
}

.pres li ._name {
  margin: 20px auto;
  padding: 20px auto;
  text-align: center;
  transition-duration: 0.3s;
  color: #ff4c4c;
  z-index: 2;
  font: 20px "Roboto-Light", Verdana, sans-serif;

  text-decoration: none; 

  -webkit-transition: box-shadow 0.5s ease;
  -moz-transition: box-shadow 0.5s ease;
  -o-transition: box-shadow 0.5s ease;
  -ms-transition: box-shadow 0.5s ease;
  transition: box-shadow 0.5s ease;
}

.pres li a:hover ._name {
    color: #FF8235;
}

.pres li img {
    margin: 20px auto;
    padding: 20px auto;
    border: 1px solid #ff4c4c;
    height: 150px;
    width: 130px;

    margin-right: 35px;
    margin-top: 35px;

  -webkit-transition: box-shadow 0.5s ease;
  -moz-transition: box-shadow 0.5s ease;
  -o-transition: box-shadow 0.5s ease;
  -ms-transition: box-shadow 0.5s ease;
  transition: box-shadow 0.5s ease;
}

.pres li a:hover:after {
  opacity: 0.5;
}

